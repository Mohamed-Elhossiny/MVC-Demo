@model List<TraineeCourseViewModel>
@{
	ViewData["Title"] = "showCourseR";
}

<h1>Show Course Result</h1>
<table class="table table-hover table-borderd table-striped">
	<tr>
		<th>Trainee Name</th>
		<th>Course Name</th>
		<th>Degree</th>
	</tr>
	@foreach (var item in Model)
	{
		<tr style="background:@item.Color;color:white">
			<td>@item.T_Name</td>
			<td>@item.C_Name</td>
			<td>@item.Degree</td>
		</tr>
	}
</table>
