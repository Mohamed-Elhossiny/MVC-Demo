	ViewData["Title"] = "Edit";
	SelectList deptList = new SelectList(ViewBag.deptList, "Id", "Name");
	SelectList courseList = new SelectList(ViewBag.courseList, "Id", "Name");
}

<h1 class="text-center fw-bolder">Edit</h1>

