@model List<Department>
@{
	ViewData["Title"] = "Index";
}

<h1>Index</h1>
<table class="table table-hover table-stripped table-bordered">
	<thead class="bg-primary">
	<th>ID</th>
	<th>Name</th>
	<th>Manager</th>
	</thead>
	<tbody>
		@foreach (Department dept in Model)
		{
			<tr>
				<td>@dept.Id</td>
				<td>@dept.Name</td>
				<td>@dept.Manager</td>
			</tr>
		}
	</tbody>
</table>
